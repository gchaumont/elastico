{
    "name": "gchaumont/elastico",
    "description": "Elasticsearch ORM for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
    {
        "name": "Guy Chaumont",
        "email": "gchaumont@me.com"
    }],
    "require":
    {
        "php": "^8.1",
        "illuminate/collections": "^9.11",
        "illuminate/support": "^9.11",
        "php-http/guzzle7-adapter": "^1.0",
        "elasticsearch/elasticsearch": "^8.2",
        "spatie/ssh": "^1.7",
        "illuminate/database": "^9.46"
    },
    "require-dev":
    {
        "illuminate/support": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/events": "^9.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Elastico\\": "src"
        }
    },
    "extra":
    {
        "laravel":
        {
            "providers": [
                "Elastico\\ElasticServiceProvider"
            ]
        }
    }
}
