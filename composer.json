{
    "name": "gchaumont/elastico",
    "description": "Elasticsearch Eloquent ORM",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Guy Chaumont",
            "email": "gchaumont@me.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/collections": "^10.0",
        "illuminate/support": "^10.0",
        "php-http/guzzle7-adapter": "^1.0",
        "elasticsearch/elasticsearch": "^8.2",
        "spatie/ssh": "^1.7",
        "illuminate/database": "^10.0"
    },
    "require-dev": {
        "illuminate/support": "^10.0",
        "illuminate/container": "^10.0",
        "illuminate/events": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Elastico\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elastico\\ElasticServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}